{
  "id": "/registry",
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "container": {
    "portMappings": [
      {
        "containerPort": 5000,
        "hostPort": 5000,
        "labels": {
          "VIP_0": "/registry:5000"
        },
        "protocol": "tcp",
        "servicePort": 10008,
        "name": "registry"
      }
    ],
    "type": "DOCKER",
    "volumes": [
      {
        "containerPath": "/var/lib/registry",
        "hostPath": "/data/apps/sports-cloud/docker/registry",
        "mode": "RW"
      },
      {
        "containerPath": "/config/docker/security",
        "hostPath": "/data/apps/sports-cloud/docker-client/security",
        "mode": "RW"
      }
    ],
    "docker": {
      "image": "registry:2.5.1",
      "forcePullImage": false,
      "privileged": false,
      "parameters": []
    }
  },
  "cpus": 0.2,
  "disk": 0,
  "env": {
    "REGISTRY_HTTP_TLS_CERTIFICATE": "/config/docker/security/domain.crt",
    "REGISTRY_HTTP_TLS_KEY": "/config/docker/security/domain.key",
    "REGISTRY_STORAGE_FILESYSTEM_ROOTDIRECTORY": "/var/lib/registry"
  },
  "acceptedResourceRoles": [
    "slave_public"
  ],
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 3,
      "portIndex": 0,
      "timeoutSeconds": 20,
      "delaySeconds": 15,
      "protocol": "TCP"
    }
  ],
  "instances": 1,
  "maxLaunchDelaySeconds": 3600,
  "mem": 256,
  "gpus": 0,
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 0,
    "minimumHealthCapacity": 0
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 300,
    "expungeAfterSeconds": 600
  },
  "constraints": []
}